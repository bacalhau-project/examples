{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject",
        "s3:ListBucket",
        "s3:GetBucketLocation",
        "s3:GetLifecycleConfiguration",
        "s3:PutLifecycleConfiguration"
      ],
      "Resource": [
        "arn:aws:s3:::expanso-databricks-ingestion-us-west-2/*",
        "arn:aws:s3:::expanso-databricks-ingestion-us-west-2",
        "arn:aws:s3:::expanso-databricks-validated-us-west-2/*",
        "arn:aws:s3:::expanso-databricks-validated-us-west-2",
        "arn:aws:s3:::expanso-databricks-enriched-us-west-2/*",
        "arn:aws:s3:::expanso-databricks-enriched-us-west-2",
        "arn:aws:s3:::expanso-databricks-aggregated-us-west-2/*",
        "arn:aws:s3:::expanso-databricks-aggregated-us-west-2",
        "arn:aws:s3:::expanso-databricks-checkpoints-us-west-2/*",
        "arn:aws:s3:::expanso-databricks-checkpoints-us-west-2",
        "arn:aws:s3:::expanso-databricks-metadata-us-west-2/*",
        "arn:aws:s3:::expanso-databricks-metadata-us-west-2"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "sts:AssumeRole"
      ],
      "Resource": [
        "arn:aws:iam::767397752906:role/expanso-databricks-unity-catalog-role"
      ]
    }
  ]
}
